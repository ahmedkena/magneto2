<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/custom_logo.svg</argument>
                <argument name="logo_height" xsi:type="string">50</argument>
                <argument name="logo_width" xsi:type="string">50</argument>
            </arguments>
        </referenceBlock>


    <referenceBlock name="header.links">
        <block class="Magento\Framework\View\Element\Html\Link" name="top-link">
          <arguments>
              <argument name="label" xsi:type="string" translate="true">LinkedIn </argument>
              <argument name="path" xsi:type="string" translate="true">https://linkedin.com/in/ahmedkena</argument>
          </arguments>
        </block>
      </referenceBlock>
    </body>
</page>